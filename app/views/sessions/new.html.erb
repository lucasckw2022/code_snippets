<h1>User Sign In</h1>
<%= form_tag sessions_path do%>
  <div class="">
    <%= label_tag :email%>
    <%= email_field_tag :email%>
  </div>
  <div class="">
    <%= label_tag :password%>
    <%= password_field_tag :password%>
  </div>
  <div class="">
    <%= label_tag :password_confirmation%>
    <%= password_field_tag :password_confirmation%>
  </div>
  <%= submit_tag "Sign In"%>
<%end%>
